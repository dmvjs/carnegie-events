    <plugin name="cordova-plugin-screen-orientation" spec="~1.4.2" />
    <plugin name="cordova-plugin-splashscreen" spec="^3.2.2" />
    <plugin name="cordova-plugin-statusbar" spec="~2.1.3" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-calendar" spec="^4.6.0">
        <variable name="CALENDAR_USAGE_DESCRIPTION" value="This app uses your calendar" />
    </plugin>
    <plugin name="cordova-plugin-compat" spec="^1.2.0" />
    <plugin name="com.bunkerpalace.cordova.YoutubeVideoPlayer" spec="git+https://github.com/JonSmart/CordovaYoutubeVideoPlayer.git" />
    <engine name="android" spec="^6.3.0" />
    <engine name="ios" spec="^4.5.4" />
</widget>
